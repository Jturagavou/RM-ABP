{
  "name": "RM-ABP",
  "description": "Resource Manager for Agent-Based Programming - iOS SwiftUI App",
  "version": "1.0.0",
  "type": "ios-swift",
  "main": "RM-ABP/RM_ABPApp.swift",
  "workspace": {
    "folders": [
      {
        "name": "iOS Source",
        "path": "./RM-ABP"
      },
      {
        "name": "Xcode Project", 
        "path": "./RM-ABP.xcodeproj"
      }
    ]
  },
  "settings": {
    "editor.defaultFormatter": "swift",
    "files.associations": {
      "*.swift": "swift"
    }
  },
  "extensions": {
    "recommendations": [
      "swift.swift-lang",
      "ms-vscode.vscode-swift"
    ]
  }
}